# Paddle Environment Variables
# Get these from your Paddle dashboard
# Make sure to use your Sandbox credentials for testing first

# Your Vendor ID from Paddle
NEXT_PUBLIC_PADDLE_VENDOR_ID="YOUR_VENDOR_ID"

# Set to 'true' for testing with the sandbox, 'false' for production
NEXT_PUBLIC_PADDLE_SANDBOX="true"

# Price IDs for your subscription plans (from your Paddle Catalog)
NEXT_PUBLIC_PADDLE_PREMIUM_YEARLY_PRICE_ID="pri_xxxxxxxxxxxxxx"
NEXT_PUBLIC_PADDLE_PREMIUM_MONTHLY_PRICE_ID="pri_xxxxxxxxxxxxxx"
NEXT_PUBLIC_PADDLE_PRO_YEARLY_PRICE_ID="pri_xxxxxxxxxxxxxx"
